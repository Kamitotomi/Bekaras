        
        
          
          <td><%= link_to post.body, post_path(post.id) %></td>
          
          <% if post.category == 0 %>
          <td>なんてしない</td>
          <% else %>
          <td>とか言わない</td>
          <% end %>
          <td><%= post.start_time %></td>
        <td>
          
                <% if post.favorited_by?(current_user) %>
                  <%= link_to post_favorites_path(post, redirect_id: 0), method: :delete, remote: true do %>
                    <span class="glyphicon glyphicon-heart"><%= post.favorites.count%></span>
                  <% end %>
                          
                  <% else %>
                          
                  <%= link_to post_favorites_path(post, redirect_id: 0), method: :post, remote: true do %>
                    <span class="glyphicon glyphicon-heart-empty"><%= post.favorites.count %></span>
                  <% end %>
                <% end %>
          </td>

          